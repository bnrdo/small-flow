<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
</head>
<body>
		<div class="row">
		<div style="" class="smf-content-header">
			<div class="smf-content-title">Cash Inflow</div>
			<div class="margin-right-15 float-right"><button type="button" class="btn btn-primary">View Records</button></div>
		</div>
		</div>
		<div class="row">
		<div class="col-lg-12">
			<div class="panel panel-default">
				<div class="panel-body">
					<div class="row">
						<div class="col-lg-6">
							<form role="form">
								<div class="form-group">
									<label>Flow from</label> <select class="form-control">
										<option>1</option>
										<option>2</option>
										<option>3</option>
										<option>4</option>
										<option>5</option>
									</select>
								</div>
								<div class="form-group">
									<label>Date</label> <input class="datepicker form-control">
								</div>
								<div class="form-group">
									<label>Amount</label> <input class="form-control">
								</div>
								<div class="form-group">
									<label>Notes</label>
									<textarea class="form-control" rows="3"></textarea>
								</div>
								<button type="button" class="btn btn-primary">Save</button>
								<button type="button" class="btn btn-primary">Clear</button>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<!-- Page JavaScript -->
	<script src="js/smf/cash-in.js"></script>
</body>
</html>